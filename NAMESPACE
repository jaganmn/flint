useDynLib(flint, .registration = TRUE)
importFrom(methods, as, coerce, initialize, new, setAs, setClass, setMethod)
exportClasses(flint, slong, ulong, fmpz, fmpq, arf, mag, arb, acb,
              nflint, nslong, nulong, nfmpz, nfmpq, narf, nmag, narb, nacb)
exportMethods(as.raw, as.logical, as.integer, as.double, as.numeric, as.complex,
              as.list, as.expression, as.vector, coerce, initialize, length)
export(flintBits,
       acb_dirichlet_lerch_phi, acb_hypgeom_2f1, acb_hypgeom_2f1_continuation)
