useDynLib(flint, .registration = TRUE)
importFrom(methods, Complex, Math, Math2, Ops, Summary, as, coerce,
           initialize, new, setAs, setClass, setMethod, show)
exportClasses(flint, slong, ulong, fmpz, fmpq, arf, mag, arb, acb)
exportMethods("+", "-", "[", "[<-", "[[", "[[<-", Complex, Math,
              Math2, Ops, Summary, as.raw, as.logical, as.integer,
              as.numeric, as.complex, as.list, as.expression, as.vector,
              coerce, format, initialize, length, log, print, show)
export(flintBits,
       flintVersion,
       flintPrec,
       flintRnd,
       arb_lambertw,
       acb_lambertw,
       arb_dirichlet_zeta,
       acb_dirichlet_zeta,
       arb_dirichlet_hurwitz,
       acb_dirichlet_hurwitz,
       acb_dirichlet_lerch_phi,
       arb_hypgeom_gamma,
       acb_hypgeom_gamma,
       arb_hypgeom_rgamma,
       acb_hypgeom_rgamma,
       arb_hypgeom_lgamma,
       acb_hypgeom_lgamma,
       acb_hypgeom_polygamma,
       arb_hypgeom_2f1,
       acb_hypgeom_2f1)
